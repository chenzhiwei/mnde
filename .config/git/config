[user]
    name    = Chen Zhiwei
    email   = zhiwei@youya.org
[core]
    editor  = vim
    pager   = less -x1,5
[merge]
    tool    = vimdiff
[color]
    diff    = auto
    status  = auto
    branch  = auto
    interactive = auto
[alias]
    st  = status
    last = log -1 HEAD
    unstage = restore --staged
    history = log -p --
[pull]
    ff = only
[push]
    default = current
[credential]
    helper = store --file=$HOME/.config/git/credentials
[init]
    defaultBranch = master
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
